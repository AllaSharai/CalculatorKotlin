<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" maxHeight="1000.0" maxWidth="1000.0" minHeight="700" minWidth="500" prefHeight="700.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
    <TextField fx:id="expressionText" alignment="CENTER_RIGHT" editable="false" prefHeight="77.0" prefWidth="500.0" text="0">
        <font>
            <Font size="35.0" />
        </font>
    </TextField>
    <GridPane alignment="CENTER" prefHeight="120.0" prefWidth="500.0" VBox.vgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <Button mnemonicParsing="false" onAction="#digitPressed" prefHeight="1000.0" prefWidth="1000.0" text="4" GridPane.rowIndex="2">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#cePressed" prefHeight="1000.0" prefWidth="1000.0" text="CE">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#cPressed" prefHeight="1000.0" prefWidth="1000.0" text="C" GridPane.columnIndex="1">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#openParenthesisPressed" prefHeight="1000.0" prefWidth="1000.0" text="(" GridPane.columnIndex="2">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#closeParenthesisPressed" prefHeight="1000.0" prefWidth="1000.0" text=")" GridPane.columnIndex="3">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#digitPressed" prefHeight="1000.0" prefWidth="1000.0" text="7" GridPane.rowIndex="1">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#digitPressed" prefHeight="1000.0" prefWidth="1000.0" text="8" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#digitPressed" prefHeight="1000.0" prefWidth="1000.0" text="9" GridPane.columnIndex="2" GridPane.rowIndex="1">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#operatorPressed" prefHeight="1000.0" prefWidth="1000.0" text="/" GridPane.columnIndex="3" GridPane.rowIndex="1">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#digitPressed" prefHeight="1000.0" prefWidth="1000.0" text="5" GridPane.columnIndex="1" GridPane.rowIndex="2">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#digitPressed" prefHeight="1000.0" prefWidth="1000.0" text="6" GridPane.columnIndex="2" GridPane.rowIndex="2">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#operatorPressed" prefHeight="1000.0" prefWidth="1000.0" text="*" GridPane.columnIndex="3" GridPane.rowIndex="2">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#digitPressed" prefHeight="1000.0" prefWidth="1000.0" text="1" GridPane.rowIndex="3">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#digitPressed" prefHeight="1000.0" prefWidth="1000.0" text="2" GridPane.columnIndex="1" GridPane.rowIndex="3">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#digitPressed" prefHeight="1000.0" prefWidth="1000.0" text="3" GridPane.columnIndex="2" GridPane.rowIndex="3">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#operatorPressed" prefHeight="1000.0" prefWidth="1000.0" text="-" GridPane.columnIndex="3" GridPane.rowIndex="3">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#digitPressed" prefHeight="1000.0" prefWidth="1000.0" text="0" GridPane.rowIndex="4">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#dotPressed" prefHeight="1000.0" prefWidth="1000.0" text="." GridPane.columnIndex="1" GridPane.rowIndex="4">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#equalsPressed" prefHeight="1000.0" prefWidth="1000.0" text="=" GridPane.columnIndex="2" GridPane.rowIndex="4">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onAction="#operatorPressed" prefHeight="1000.0" prefWidth="1000.0" text="+" GridPane.columnIndex="3" GridPane.rowIndex="4">
            <font>
                <Font size="35.0" />
            </font>
        </Button>
    </GridPane>
</VBox>
