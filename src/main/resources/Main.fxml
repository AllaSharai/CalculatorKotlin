<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="700.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controller.Controller">
    <TextField alignment="CENTER_RIGHT" editable="false" prefHeight="77.0" prefWidth="500.0" text="0"
               fx:id="expressionText">
        <font>
            <Font size="35.0"/>
        </font>
    </TextField>
    <GridPane alignment="CENTER" prefHeight="120.0" prefWidth="500.0" VBox.vgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="4" GridPane.rowIndex="2"
                onAction="#digitPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="CE" onAction="#cePressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="C" GridPane.columnIndex="1"
                onAction="#cPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="(" GridPane.columnIndex="2"
                onAction="#openParenthesisPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text=")" GridPane.columnIndex="3"
                onAction="#closeParenthesisPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="7" GridPane.rowIndex="1"
                onAction="#digitPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="8" GridPane.columnIndex="1"
                GridPane.rowIndex="1" onAction="#digitPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="9" GridPane.columnIndex="2"
                GridPane.rowIndex="1" onAction="#digitPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="/" GridPane.columnIndex="3"
                GridPane.rowIndex="1" onAction="#operatorPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="5" GridPane.columnIndex="1"
                GridPane.rowIndex="2" onAction="#digitPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="6" GridPane.columnIndex="2"
                GridPane.rowIndex="2" onAction="#digitPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="*" GridPane.columnIndex="3"
                GridPane.rowIndex="2" onAction="#operatorPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="1" GridPane.rowIndex="3"
                onAction="#digitPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="2" GridPane.columnIndex="1"
                GridPane.rowIndex="3" onAction="#digitPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="3" GridPane.columnIndex="2"
                GridPane.rowIndex="3" onAction="#digitPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="-" GridPane.columnIndex="3"
                GridPane.rowIndex="3" onAction="#operatorPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="0" GridPane.rowIndex="4"
                onAction="#digitPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="." GridPane.columnIndex="1"
                GridPane.rowIndex="4" onAction="#dotPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="=" GridPane.columnIndex="2"
                GridPane.rowIndex="4">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
        <Button mnemonicParsing="false" prefHeight="1000.0" prefWidth="1000.0" text="+" GridPane.columnIndex="3"
                GridPane.rowIndex="4" onAction="#operatorPressed">
            <font>
                <Font size="35.0"/>
            </font>
        </Button>
    </GridPane>
</VBox>
